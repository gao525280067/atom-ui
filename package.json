{
  "name": "react-atom-ui",
  "version": "0.0.1",
  "description": "细粒度的React UI组件库，践行细粒度的组合优于多功能的封装理念，分离状态、样式、组件等原子，按需引用，灵活组合，减小打包体积！",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "alpha": "npm run build && npm publish --tag=alpha",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:gao525280067/atom-ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "hooks",
    "atom"
  ],
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "1.1.0-beta.24",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-tinker": "^0.5.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "chengaofeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gao525280067/atom-ui/issues"
  },
  "homepage": "https://github.com/gao525280067/atom-ui#readme",
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ]
}
